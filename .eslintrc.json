{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier", "import"],
  "rules": {
    "prettier/prettier": ["error"],
    "import/order": [
      "error",
      {
        "groups": ["builtin", "external", "internal", "parent", "sibling", "index"],
        "alphabetize": {
          "order": "asc",
          "caseInsensitive": true
        }
      }
    ]
  }
}
